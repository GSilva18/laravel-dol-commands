{
    "name": "gsilva18/laravel-dol-commands",
    "description": "Artisan commands to Laravel apps that utilizes Domain Oriented Laravel.",
    "homepage": "https://github.com/gsilva18/laravel-ddd-commands",
    "license": "MIT",
    "authors": [
        {
            "name": "Genilson Silva",
            "email": "genilson_silva18@hotmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^7.0|^8.0",
        "spatie/data-transfer-object":"^3.7"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dol\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dol\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dol\\Providers\\DomainServiceProvider"
            ]
        }
    }
}
